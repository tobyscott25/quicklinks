{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Quicklinks Configuration",
    "type": "object",
    "properties": {
        "redirects": {
            "type": "object",
            "description": "Map shortcuts to URLs",
            "items": {
                "type": "object",
                "properties": {
                    "*": {
                        "type": "string",
                        "description": "Map a shortcut to a URL"
                    }
                },
                "required": [
                    "*"
                ]
            }
        }
    },
    "required": [
        "redirects"
    ]
}